{
	"name": {{ package_name | to_json }},
	"version": "0.1.0",
	"description": {{ project_description | to_json }},
	"main": "main.js",
	"scripts": {
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"dev": "node esbuild.config.mjs",
		"format": "biome format --write",
		"lint": "biome check",
		"preinstall": "npx only-allow pnpm",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": {{ author | to_json }},
	{% if author_url -%}
	"url": {{ author_url | to_json }},
	{% endif -%}
	"repository": "github:{{ github_repo }}",
	"homepage": "https://github.com/{{ github_repo }}#readme",
	"bugs": "https://github.com/{{ github_repo }}/issues",
	{% if funding_url -%}
	"funding": {{ funding_url | to_json }},
	{% endif -%}
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "2.0.5",
		"@types/node": "^22.14.1",
		"builtin-modules": "5.0.0",
		"esbuild": "^0.25.2",
		"obsidian": "^1.8.7",
		"obsidian-typings": "^2.42.0",
		"tslib": "2.8.1",
		"typescript": "5.8.3"
	},
	"packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971"
}
